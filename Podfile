# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'SocialLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
pod 'FBSDKCoreKit'
pod 'FBSDKShareKit'
pod 'FBSDKLoginKit'

pod 'Firebase/Auth'
pod 'GoogleSignIn'
  # Pods for SocialLogin

end
